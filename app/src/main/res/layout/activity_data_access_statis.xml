<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_data_access_statis"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/divide_gray_color"
    tools:context=".fragment.TodayAccessFragment">

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:id="@+id/activity_data_access_1"
        android:background="@drawable/rounded_shape"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="@dimen/ap_et_margin_start"
        android:layout_marginRight="@dimen/ap_et_margin_start"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >
        <TextView
            android:id="@+id/tv_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/ap_text_size"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="@dimen/ap_et_margin_start"
            android:text="9门数量"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
           >
        </TextView>

        <TextView
            android:id="@+id/tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:textSize="@dimen/ap_text_size"
            android:layout_marginLeft="@dimen/ap_et_margin_start"
            android:text="300"
            app:layout_constraintTop_toBottomOf="@+id/tv_1"
            app:layout_constraintStart_toStartOf="@+id/tv_1"
            app:layout_constraintEnd_toEndOf="@+id/tv_1">
        </TextView>

        <TextView
            android:id="@+id/tv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/ap_text_size"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="@dimen/ap_et_margin_start"
            android:text="通信人次"
            app:layout_constraintTop_toBottomOf="@+id/tv2"
            app:layout_constraintStart_toStartOf="@+id/tv2"
            app:layout_constraintEnd_toEndOf="@+id/tv2">
        </TextView>

        <TextView
            android:id="@+id/tv4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/ap_text_size"
            android:layout_marginLeft="100dp"
            android:text="200"
            android:textColor="@color/red"
            app:layout_constraintTop_toTopOf="@+id/tv2"
            app:layout_constraintBottom_toBottomOf="@+id/tv2"
            app:layout_constraintStart_toEndOf="@id/tv2"
          >
        </TextView>

        <TextView
            android:id="@+id/tv5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/ap_text_size"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="@dimen/ap_et_margin_start"
            android:text="正常通行"
            app:layout_constraintTop_toBottomOf="@+id/tv4"
            app:layout_constraintStart_toStartOf="@+id/tv4"
            app:layout_constraintEnd_toEndOf="@+id/tv4">
        </TextView>

        <TextView
            android:id="@+id/tv6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:textSize="@dimen/ap_text_size"
            android:layout_marginLeft="100dp"
            android:text="100"
            android:textColor="@color/red"
            app:layout_constraintTop_toTopOf="@+id/tv4"
            app:layout_constraintBottom_toBottomOf="@+id/tv4"
            app:layout_constraintStart_toEndOf="@id/tv4"
            >
        </TextView>

        <TextView
            android:id="@+id/tv7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/ap_text_size"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="@dimen/ap_et_margin_start"
            android:text="陌生人通行"
            app:layout_constraintTop_toBottomOf="@+id/tv6"
            app:layout_constraintStart_toStartOf="@+id/tv6"
            app:layout_constraintEnd_toEndOf="@+id/tv6">
        </TextView>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:id="@+id/activity_data_access_2"
        android:background="@drawable/rounded_shape"
        android:layout_marginLeft="@dimen/ap_et_margin_start"
        android:layout_marginRight="@dimen/ap_et_margin_start"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/activity_data_access_1"
        app:layout_constraintStart_toStartOf="@+id/activity_data_access_1"
        app:layout_constraintEnd_toEndOf="@+id/activity_data_access_1"
        >

    </RelativeLayout>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:id="@+id/activity_data_access_3"
        android:background="@color/white"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/activity_data_access_2"
        app:layout_constraintStart_toStartOf="@+id/activity_data_access_2"
        app:layout_constraintEnd_toEndOf="@+id/activity_data_access_2"
        >

    </RelativeLayout>



    
</androidx.constraintlayout.widget.ConstraintLayout>